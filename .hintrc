{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off",
        "duplicate-id-active": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/text-alternatives": "off",
    "no-inline-styles": "off",
    "button-type": "off",
    "axe/name-role-value": "off",
    "typescript-config/strict": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "disown-opener": "off"
  }
}